<div class="modal empty-modal" id='empty-modal-<%= resource %>'>
  <label for="modal-<%= resource %>" style='display:none'>
    <div class="modal-<%= resource %>"><%= resource %></div>
  </label>
  <input class="modal-state" id="modal-<%= resource %>" type="checkbox" />
  <div class="modal-window">
    <div class="modal-inner">
      <label class="modal-close" for="modal-<%= resource %>"></label>
      <div class='modal-content' id="modal-content-<%= resource %>"></div>
    </div>
  </div>
</div>


